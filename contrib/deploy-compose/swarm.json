"service_name": "postgres",
"components": [
    {
        "component_name": "postgres",
        "image": "postgres:9.4",
        "ports": [5432],
        "env": {
            "POSTGRES_PASSWORD": "your_password"
        },
        "volumes": [{
            "path": "/var/lib/postgresql/data",
            "size": "2 GB"
        }]
    }
]
